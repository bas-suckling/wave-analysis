    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "chartist": "^0.11.4",
    "chartist-plugin-axistitle": "0.0.7",
